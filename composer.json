{
    "name": "helman/sift-php",
    "description": "Sift PHP version ported from siftjs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Helman Budianto",
            "email": "helman@heldes.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "anahkiasen/underscore-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Sift\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sift\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
